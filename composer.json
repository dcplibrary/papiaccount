{
  "name": "dcplibrary/papiaccount",
  "require": {
    "php": "^8.3",
    "illuminate/support": "^9.0||^10.0||^11.0||^12.0",
    "blashbrook/papiclient": "^v2.4.8",
    "livewire/flux": "^2.5"
  },
  "type": "library",
  "description": "Laravel package for PAPIAccount",
  "keywords": [
    "laravel",
    "package",
    "papi",
    "account"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Brian Lashbrook",
      "email": "blashbrook@dcplibrary.org"
    }
  ],
  "homepage": "https://www.dcplibrary.org",
  "autoload": {
    "psr-4": {
      "Dcplibrary\\PAPIAccount\\": "src/",
      "Dcplibrary\\PAPIAccount\\App\\": "src/app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dcplibrary\\PAPIAccount\\Tests\\": "src/tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Dcplibrary\\PAPIAccount\\App\\Providers\\PAPIAccountServiceProvider"
      ],
      "aliases": {
        "PAPIAccount": "Dcplibrary\\PAPIAccount\\Facades\\PAPIAccount"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "php-cs-fixer fix",
    "analyse": "phpstan analyse --memory-limit=512M"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "larastan/larastan": "^3.7",
    "orchestra/testbench": "^10.6"
  }
}
