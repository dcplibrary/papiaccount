{
  "name": "dcplibrary/papiaccount",
  "require": {
    "php": "^8.3",
    "livewire/flux": "^2.4",
    "blashbrook/papiclient": "^2.4.4"
  },
  "type": "library",
  "description": "Laravel package for PAPIAccount",
  "keywords": [
    "laravel",
    "package",
    "papi",
    "account"
  ],
  "license": "MIT",
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.16",
    "phpstan/phpstan": "^2.1",
    "friendsofphp/php-cs-fixer": "^3.88",
    "phpunit/phpunit": "^12.3"
  },
  "authors": [
    {
      "name": "Brian Lashbrook",
      "email": "blashbrook@dcplibrary.org"
    }
  ],
  "homepage": "https://www.dcplibrary.org",
  "autoload": {
    "psr-4": {
      "Dcplibrary\\PAPIAccount\\": "src/",
      "Dcplibrary\\PAPIAccount\\App\\": "src/app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dcplibrary\\PAPIAccount\\Tests\\": "src/Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Dcplibrary\\PAPIAccount\\App\\Providers\\PAPIAccountServiceProvider"
      ],
      "aliases": {
        "PAPIAccount": "Dcplibrary\\PAPIAccount\\Facades\\PAPIAccount"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "php-cs-fixer fix",
    "analyse": "phpstan analyse"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
